<Window x:Class="VetClin.DailyReportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VetClin"
        mc:Ignorable="d"
        Title="DailyReportWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="10,32,20,20">
            <TextBlock x:Name="TotalAppointmentsTextBlock" FontSize="16" Margin="0 0 0 10"/>
            <TextBlock x:Name="TotalServicesCostTextBlock" FontSize="16"/>
        </StackPanel>
    </Grid>
</Window>
